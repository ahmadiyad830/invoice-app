<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/menu_home">

    <fragment
        android:id="@+id/menu_search"
        android:name="soft.mahmod.yourreceipt.view_fragment.main.FragmentSearch"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/menu_profile"
        android:name="soft.mahmod.yourreceipt.view_fragment.main.FragmentProfile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/menu_home"
        android:name="soft.mahmod.yourreceipt.view_fragment.main.FragmentReceipt"
        android:label="FragmentHome"
        tools:layout="@layout/fragment_receipt" />
    <fragment
        android:id="@+id/edit_store"
        android:name="soft.mahmod.yourreceipt.view_fragment.edit_account.FragmentEditAccount"
        android:label="fragment_edit_account"
        tools:layout="@layout/fragment_edit_account" />
    <fragment
        android:id="@+id/change_password"
        android:name="soft.mahmod.yourreceipt.view_fragment.edit_account.FragmentChangePassword"
