        tools:layout="@layout/fragment_change_password" />
    <fragment
        android:id="@+id/menu_item"
        android:name="soft.mahmod.yourreceipt.view_fragment.main.FragmentItem"
        android:label="FragmentItem"
        tools:layout="@layout/layout_items">
        <action
            android:id="@+id/action_menu_item_to_fragmentCreateItem2"
            app:destination="@id/fragmentCreateItem2" />
    </fragment>
    <fragment
        android:id="@+id/menu_client"
        android:name="soft.mahmod.yourreceipt.view_fragment.main.FragmentClient"
        android:label="FragmentClient"
        tools:layout="@layout/fragment_add_client">
        <action
            android:id="@+id/action_menu_client_to_fragmentCreateClient2"
            app:destination="@id/fragmentCreateClient2" />
    </fragment>
    <fragment
        android:id="@+id/fragmentCreateItem2"
        android:name="soft.mahmod.yourreceipt.view_fragment.add_receipt.FragmentCreateItem"
        android:label="FragmentCreateItem"
        tools:layout="@layout/fragment_create_item" />
    <fragment
        android:id="@+id/fragmentCreateClient2"
        android:name="soft.mahmod.yourreceipt.view_fragment.add_receipt.FragmentCreateClient"
        android:label="FragmentCreateClient"
        tools:layout="@layout/fragment_create_client" />
